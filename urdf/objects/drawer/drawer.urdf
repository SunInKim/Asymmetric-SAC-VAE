      <geometry>
        <mesh filename="mesh/drawer_main88.obj" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="tray_material">
        <color rgba="0.9 0.10 0.68 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="mesh/drawer_main88.obj" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>
  </link>

  <joint name="fix2" type="fixed">
    <parent link="bot2"/>
    <child link="cover"/>
    <origin rpy="0 0 0" xyz="0 0 0.075" />
  </joint>

  <link name="cover">

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
